{
  "swagger": "2.0",
  "info": {
    "title": "InterSystems IRIS REST API ingestion demo (client)",
    "description": "A simple REST interface to evaluate the ingestion speed of an InterSystems IRIS REST API",
    "contact": {
      "name": "InterSystems Community",
      "url": "https://openexchange.intersystems.com/"
    },
    "license": {
      "name": "MIT license",
      "url": "https://github.com/dootje-2004/intersystems-rest-demo/blob/main/LICENSE.txt"
    },
    "version": "0.0.1"
  },
  "host": "localhost:9090",
  "basePath": "/demo",
  "schemes": [
    "http"
  ],
  "paths": {
    "/request/count": {
      "get": {
        "description": "Returns the number of test requests.",
        "produces": [
          "text/plain"
        ],
        "responses": {
          "200": {
            "description": "Value is returned normally."
          }
        },
        "operationId": "getRequestCount"
      }
    },
    "/request/count/{count}": {
      "put": {
        "description": "Sets the number of test requests.",
        "parameters": [
          {
            "in": "path",
            "type": "integer",
            "name": "count",
            "required": true
          }
        ],
        "responses": {
          "204": {
            "description": "Value has been set."
          }
        },
        "operationId": "setRequestCount"
      }
    },
    "/request/size": {
      "get": {
        "description": "Returns the payload size of a test request.",
        "produces": [
          "text/plain"
        ],
        "responses": {
          "200": {
            "description": "Value is returned normally."
          }
        },
        "operationId": "getRequestSize"
      }
    },
    "/request/size/{size}": {
      "put": {
        "description": "Sets the payload size of each test request.",
        "parameters": [
          {
            "in": "path",
            "type": "integer",
            "name": "size",
            "required": true
          }
        ],
        "responses": {
          "204": {
            "description": "Value has been set."
          }
        },
        "operationId": "setRequestSize"
      }
    },
    "/syncmethod": {
      "get": {
        "description": "Returns the synchronization method.",
        "produces": [
          "text/plain"
        ],
        "responses": {
          "200": {
            "description": "Value is returned normally."
          }
        },
        "operationId": "getSyncMethod"
      }
    },
    "/syncmethod/{method}": {
      "put": {
        "description": "Sets the synchronization method.",
        "parameters": [
          {
            "in": "path",
            "type": "string",
            "name": "method",
            "required": true
          }
        ],
        "responses": {
          "204": {
            "description": "Value has been set."
          }
        },
        "operationId": "setSyncMethod"
      }
    },
    "/server/delay": {
      "get": {
        "description": "Returns the server delay (ms).",
        "produces": [
          "text/plain"
        ],
        "responses": {
          "200": {
            "description": "Value is returned normally."
          }
        },
        "operationId": "getServerDelay"
      }
    },
    "/server/delay/{delay}": {
      "put": {
        "description": "Sets the server delay (ms).",
        "parameters": [
          {
            "in": "path",
            "type": "integer",
            "name": "delay",
            "required": true
          }
        ],
        "responses": {
          "204": {
            "description": "Value has been set."
          }
        },
        "operationId": "setServerDelay"
      }
    },
    "/server/poolsize": {
      "get": {
        "description": "Returns the poolsize of the server's Business Process.",
        "produces": [
          "text/plain"
        ],
        "responses": {
          "200": {
            "description": "Value is returned normally."
          }
        },
        "operationId": "getServerPoolsize"
      }
    },
    "/server/poolsize/{poolsize}": {
      "put": {
        "description": "Sets the poolsize of the server's Business Process",
        "parameters": [
          {
            "in": "path",
            "type": "integer",
            "name": "poolsize",
            "required": true
          }
        ],
        "responses": {
          "204": {
            "description": "Value has been set"
          }
        },
        "operationId": "setServerPoolsize"
      }
    },
    "/run-test": {
      "put": {
        "description": "Initiates a test on the server.",
        "produces": [
          "text/plain"
        ],
        "responses": {
          "200": {
            "description": "Test has been started."
          }
        },
        "operationId": "startTest"
      }
    },
    "/test/status": {
      "get": {
        "description": "Returns test status (BUSY or DONE).",
        "produces": [
          "text/plain"
        ],
        "responses": {
          "200": {
            "description": "Status returned normally."
          }
        },
        "operationId": "getTestStatus"
      }
    },
    "/data/series": {
      "get": {
        "description": "Returns the time-related data series.",
        "produces": [
          "application/json"
        ],
        "responses": {
          "200": {
            "description": "Data returned normally."
          }
        },
        "operationId": "getDataSeries"
      }
    },
    "/data/networktime": {
      "get": {
        "description": "Returns the average network time for the test.",
        "produces": [
          "text/plain"
        ],
        "responses": {
          "200": {
            "description": "Value is returned normally."
          }
        },
        "operationId": "getAverageNetworkTime"
      }
    },
    "/data/servertime": {
      "get": {
        "description": "Returns the average server processing time for the test.",
        "produces": [
          "text/plain"
        ],
        "responses": {
          "200": {
            "description": "Value is returned normally."
          }
        },
        "operationId": "getAverageServerTime"
      }
    }
  }
}
